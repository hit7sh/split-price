
entity Person {
	personName String,
	name String,
   	personId Integer,
}

entity Transaction {
    fromUserName String,
    amount Integer,
    userId Integer,
    status Status
}
enum Status {
	BORROW,
    OWE
}

relationship OneToMany {
	Person to Transaction{transactions}
}
dto Person with mapstruct
service all with serviceImpl

